{
  "name": "OneZap",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev -- --turbo",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "cd apps/web && drizzle-kit generate:mysql",
    "db:push": "cd apps/web && drizzle-kit push:mysql"
  },
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "turbo": "^1.10.12"
  }
}
